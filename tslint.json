{
  "extends": "tslint:recommended",
  "rules": {
    "no-var-requires": false,
    "no-console": false,
    "no-empty-interface": false,
    "no-namespace": false,
    "no-shadowed-variable": false,
    "max-classes-per-file": false,
    "callable-types": false,
    "no-implicit-dependencies": true
  }
}
